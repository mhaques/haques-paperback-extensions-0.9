{
  "name": "kakarotextension",
  "description": "Kakarot extensions for Paperback",
  "author": "Karrot",
  "keywords": [
    "Paperback",
    "extensions"
  ],
  "version": "0.9.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karrot0/KakarotExtension.git"
  },
  "bugs": {
    "url": "https://github.com/karrot0/KakarotExtension/issues"
  },
  "homepage": "https://github.com/karrot0/KakarotExtension#readme",
  "repositoryName": "KakarotExtension",
  "repositoryDescription": "Kakarot extensions for Paperback",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "format": "prettier --write '**/*{.md,.yml,.yaml,.json,.js,.ts}'",
    "format:check": "prettier --check '**/*{.md,.yml,.yaml,.json,.js,.ts}'",
    "bundle": "paperback-cli bundle",
    "serve": "paperback-cli serve",
    "logcat": "paperback-cli logcat",
    "dev": "paperback-cli serve -w -p 3002",
    "test": "tsc --project tsconfig.json --noEmit && npm run lint:check && npm run format:check && npm run bundle",
    "prepare": "husky",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@paperback/toolchain": "^1.0.0-alpha.52",
    "@paperback/types": "^1.0.0-alpha.52",
    "@types/eslint": "^9.6.1",
    "cheerio": "^1.0.0",
    "eslint": "^9.25.0",
    "fastest-levenshtein": "^1.0.16",
    "husky": "^9.1.7",
    "moment": "^2.30.1",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.5.3",
    "stemmer": "^2.0.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.30.1"
  },
  "dependencies": {
    "@types/node": "^22.13.0",
    "cheerio": "^1.0.0",
    "jsdom": "^26.0.0",
    "require-from-string": "^2.0.2"
  }
}
